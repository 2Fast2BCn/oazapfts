{
  "name": "oazapfts",
  "version": "1.5.0",
  "description": "OpenApi TypeScript client generator",
  "main": "lib/index.js",
  "bin": {
    "oazapfts": "./lib/cli.js"
  },
  "scripts": {
    "test": "npm run generate-demo && jest",
    "build": "tsc",
    "generate-demo": "npm run prepare && ./lib/cli.js ./demo/petstore.json ./demo/api.ts",
    "prepare": "npm run build && chmod +x ./lib/cli.js"
  },
  "keywords": [
    "openapi",
    "typescript"
  ],
  "author": "Felix Gnass",
  "license": "MIT",
  "devDependencies": {
    "@loopback/openapi-v3-types": "^1.2.1",
    "@types/got": "^9.6.7",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.4",
    "form-data": "^2.5.1",
    "jest": "^24.9.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0"
  },
  "dependencies": {
    "got": "^9.6.0",
    "lodash": "^4.17.15",
    "typescript": "^3.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "ts",
      "d.ts"
    ],
    "restoreMocks": true,
    "transform": {
      "\\.ts$": "ts-jest"
    }
  }
}

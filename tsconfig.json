{
  "compilerOptions": {
    "outDir": "lib",
    "target": "esnext",
    "sourceMap": true,
    "declaration": true,
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "baseUrl": ".",
    "paths": {
      "oazapfts/lib/*": ["src/*"]
    }
  },
  "include": ["src", "demo"],
  "exclude": ["src/codegen/ApiStub.ts"]
}
